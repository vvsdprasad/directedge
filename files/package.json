{
  "name": "directedge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "copy-404": "cp dist/index.html dist/404.html",
    "copy-cname": "cp CNAME dist/CNAME",
    "predeploy": "npm run build && npm run copy-404 && npm run copy-cname",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "lucide-react": "^0.290.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^5.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.4.8",
    "vite": "^5.0.0",
    "vitest": "^0.34.2"
  }
}
